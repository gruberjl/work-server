{
  "presets": [
    "@babel/preset-env"
  ],
  "plugins": [
    [
      "module-resolver", {
        "root": ["./src"],
        "alias": {
          "lib": "./src/lib"
        }
      }
    ],
    "@babel/plugin-proposal-export-default-from",
    "@babel/plugin-proposal-class-properties"
  ],
  "ignore": [
    "./secret.json"
  ]
}
